{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: scripts",
            "type": "debugpy",
            "request": "launch",
            "module": "scripts.${fileBasenameNoExtension}"
        },
        {
            "name": "Python Debugger: test",
            "type": "debugpy",
            "request": "launch",
            "module": "tests.${fileBasenameNoExtension}"
        },
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        }
    ]
}